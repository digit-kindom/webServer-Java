<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.async.digitkingdom.mapper.UserMapper">
    <select id="getUserByName" resultType="com.async.digitkingdom.entity.User">
        select * from device.auth_user where username = #{username}
    </select>
</mapper>